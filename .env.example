### Copy this file to .env and fill in secrets locally. Do NOT commit your .env

# Dataset selection: path or known name (flickr8k | flickr8k-subset)
MM_DATASET=flickr8k-subset

# Image backend selection: flux | stable-diffusion | nano-banana | deepinfra | freepik
IMAGE_BACKEND=flux

# Flux / OpenAI-style images
FLUX_API_KEY=YOUR_FLUX_API_KEY
FLUX_MODEL=gpt-image-1

# Stability AI
STABILITY_API_KEY=YOUR_STABILITY_API_KEY
STABILITY_MODEL=sd3

# Hugging Face
HF_API_KEY=YOUR_HF_API_KEY
HF_SD_MODEL=stabilityai/stable-diffusion-xl-base-1.0

# Nano-Banana / Gemini image API
NANO_BANANA_API_KEY=YOUR_GEMINI_API_KEY
NANO_BANANA_MODEL=gemini-2.5-flash-image

# DeepInfra
DEEP_INFRA_API_KEY=YOUR_DEEP_INFRA_API_KEY

# Freepik
FREEPIK_API_KEY=YOUR_FREEPIK_API_KEY
